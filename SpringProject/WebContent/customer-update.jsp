        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
  </head>
  <body>
    <div id="all">
      <%
      	String email = (String) session.getAttribute("email");
      	if(email != null ) {
      %>
	  <jsp:include page="top-nav.jsp"></jsp:include>	
      <div id="heading-breadcrumbs">
        <div class="container">
          <div class="row d-flex align-items-center flex-wrap">
            <div class="col-md-7">
              <h1 class="h2">New Account / Sign In</h1>
            </div>
            <div class="col-md-5">
              <ul class="breadcrumb d-flex justify-content-end">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item active">New Account / Sign In</li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <div id="content">
        <div class="container">
          <div class="row">
          	<div class="col-lg-3">              
            </div>          
            <div class="col-lg-6">
              <div class="box">
                <h2 class="text-uppercase">Update Info</h2>                
                <hr>
                <form action="member-update.do" method="post">
                  <div class="form-group">
                    <label for="email-login">Email</label>
                    <input id="email-login" name="email" value="<%= email %>"type="text" class="form-control">
                  </div>
                  <div class="form-group">
                    <label for="name-login">Name</label>
                    <input id="name-login" name="name" type="text" class="form-control">
                  </div>                  
                  <!-- 
                  <div class="form-group">
                    <label for="password-login">Password</label>
                    <input id="password-login" name="pw" type="password" class="form-control">
                  </div>
                   -->
                  <div class="form-group">
                    <label for="phone-login">Phone</label>
                    <input id="phone-login" name="phone" type="text" class="form-control">
                  </div>
                  <div class="text-center">
                    <button type="submit" class="btn btn-template-outlined"><i class="fa fa-user-md"></i> Update</button>
                  </div>
                </form>
              </div>
            </div>
            <div class="col-lg-3">              
            </div>
          </div>
        </div> 
      </div>
      <%
      	}
      	else
      		response.sendRedirect("customer-login.jsp");
      %>
      <jsp:include page="getit-footer.jsp"></jsp:include>
